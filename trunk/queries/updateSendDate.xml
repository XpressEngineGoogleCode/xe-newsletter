<query id="updateSendDate" action="update">
    <tables>
        <table name="newsletter" />
    </tables>
    <columns>
        <column name="send_date" var="send_date" />
    </columns>
    <conditions>
        <condition operation="equal" column="news_srl" var="news_srl" notnull="notnull" />
    </conditions>
</query>