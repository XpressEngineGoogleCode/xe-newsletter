<query id="updateNewsletter" action="update">
    <tables>
        <table name="newsletter" />
    </tables>
    <columns>
        <column name="title" var="title" />
        <column name="content" var="content" />
        <column name="members_list_to" var="members_list_to" />
        <column name="members_list_cc" var="members_list_cc" />
        <column name="members_list_bcc" var="members_list_bcc" />
        <column name="file_attach" var="file_attach" />
        <column name="update_date" var="update_date" />
        <column name="send_date" var="send_date" />
    </columns>
    <conditions>
        <condition operation="equal" column="news_srl" var="news_srl" notnull="notnull" />
    </conditions>
</query>